{
    "name": "chapdel/scarabee",
    "description": "Laravel Free Ray debugger alternative",
    "keywords": [
        "Ray",
        "laravel",
        "Errors",
        "Debuging"
    ],
    "homepage": "https://github.com/chapdel/scarabee",
    "license": "MIT",
    "authors": [
        {
            "name": "Chapdel KAMGA",
            "email": "chapdel.kamga2@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "spatie/backtrace": "^1.2",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Chapdel\\scarabee\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Chapdel\\scarabee\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chapdel\\scarabee\\scarabeeServiceProvider"
            ],
            "aliases": {
                "scarabee": "Chapdel\\scarabee\\scarabeeFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
